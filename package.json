{
  "name": "tj-personal-site",
  "private": true,
  "version": "1.0.0",
  "description": "A starter repository for a blog web site using the Eleventy static site generator.",
  "scripts": {
    "build": "npm-run-all build:assets build:html",
    "build:html": "eleventy",
    "build:assets": "webpack --progress",
    "watch": "npm-run-all --parallel watch:assets watch:html",
    "watch:html": "eleventy --serve",
    "watch:assets": "webpack --watch --progress --env.NODE_ENV=local",
    "start": "npm run watch",
    "debug": "DEBUG=* eleventy"
  },
  "license": "MIT",
  "homepage": "https://tj.ie",
  "devDependencies": {
    "@11ty/eleventy": "^0.10.0",
    "@11ty/eleventy-navigation": "^0.1.5",
    "@11ty/eleventy-plugin-rss": "^1.0.7",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "@fullhuman/postcss-purgecss": "^2.1.2",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.5.2",
    "cssnano": "^4.1.10",
    "file-loader": "^6.0.0",
    "html-minifier": "^4.0.0",
    "html-webpack-plugin": "^4.2.0",
    "luxon": "^1.23.0",
    "markdown-it": "^10.0.0",
    "markdown-it-anchor": "^5.2.7",
    "mini-css-extract-plugin": "^0.9.0",
    "npm-run-all": "^4.1.5",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "tailwind-vertical-rhythm": "^1.1.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "workbox-webpack-plugin": "^5.1.2"
  },
  "dependencies": {
    "instant.page": "^3.0.0",
    "netlify-plugin-search-index": "^0.1.0",
    "tailwindcss": "^1.3.4"
  }
}
