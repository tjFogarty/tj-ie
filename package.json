{
  "name": "tj-personal-site",
  "private": true,
  "version": "1.0.0",
  "description": "Personal website.",
  "type": "module",
  "scripts": {
    "clean": "rimraf dist",
    "debug": "DEBUG=* eleventy",
    "dev": "NODE_ENV=development npm-run-all clean --parallel dev:*",
