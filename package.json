  "homepage": "https://tj.ie",
  "devDependencies": {
    "@11ty/eleventy": "3.0.0-alpha.5",
    "@11ty/eleventy-img": "4.0.2",
    "@11ty/eleventy-navigation": "0.3.5",
    "@11ty/eleventy-plugin-rss": "1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "5.0.0",
    "clean-css": "5.3.3",
    "markdown-it": "14.1.0",
    "markdown-it-anchor": "8.6.7",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.5"
  },
  "dependencies": {
    "html-minifier": "^4.0.0"
  }
}