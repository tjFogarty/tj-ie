---
permalink: /tags/
layout: layouts/home.njk
---

<article>
<div class="c-hero c-hero--small">
        <div class="o-wrapper">
            <div class="c-hero__content">
                <h1 class="c-hero__title">
                    <span class="c-hero__prompt" aria-hidden="true">></span
                    >Tags
                </h1>
            </div>
        </div>
    </div>

<div class="o-wrapper">
<div class="c-content">
  {% for tag in collections.tagList %}
    {% set tagUrl %}/tags/{{ tag }}/{% endset %}
    <a href="{{ tagUrl | url }}" class="c-tag">{{ tag }}</a></li>
  {% endfor %}
  </div>
  </div>
</article>
